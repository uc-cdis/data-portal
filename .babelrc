{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 2%, not dead",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "dev-expression",
    "transform-class-properties",
    [
      "relay",
      {
        "compat": true,
        "schema": "data/schema.json"
      }
    ],
    [
      "@babel/transform-runtime",
      {
        "corejs": 3
      }
    ],
    [
      "@babel/plugin-proposal-object-rest-spread",
      {
        "loose": true,
        "useBuiltIns": true
      }
    ],
    ["@babel/plugin-transform-modules-commonjs"]
  ]
}
